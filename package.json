{
  "name": "ng-seed",
  "version": "1.0.0",
  "description": "angular seed app",
  "main": "src/index.html",
  "scripts": {
    "clean": "rm -rf dist coverage",
    "ng": "ng",
    "build": "npm run build-shared && npm run build-web && npm run build-handset",
    "build-shared": "ng build app-shared --prod",
    "build-web": "ng build web --prod --base-href=/ng-seed/web/",
    "build-handset": "ng build handset --prod --base-href=/ng-seed/handset/",
    "watch-web": "ng build web --watch --base-href=/ng-seed/web/",
    "watch-handset": "ng build handset --watch --base-href=/ng-seed/handset/",
    "lint-shared": "ng lint app-shared",
    "lint-web": "ng lint web",
    "lint-handset": "ng lint handset",
    "start-web": "ng serve web --host 0.0.0.0 --base-href=/ng-seed/web/",
    "start-handset": "ng serve handset --host 0.0.0.0 --base-href=/ng-seed/handset/",
    "test-web": "ng test web --watch",
    "test-handset": "ng test handset --watch",
    "test-web-coverage": "ng test web --code-coverage",
    "test-handset-coverage": "ng test handset --code-coverage",
    "e2e-web": "ng e2e web --webdriver-update false",
    "e2e-handset": "ng e2e handset --webdriver-update false",
    "webdriver-update": "webdriver-manager update --proxy http://127.0.0.1:10089"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beginor/ng-seed.git"
  },
  "keywords": [
    "angular",
    "seed",
    "angular-cli"
  ],
  "author": "beginor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beginor/ng-seed/issues"
  },
  "homepage": "https://github.com/beginor/ng-seed#readme",
  "dependencies": {
    "@angular/animations": "~10.1.0",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/forms": "~10.1.0",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/router": "~10.1.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2",
    "@angular/localize": "~10.1.0",
    "bootstrap": "^4.5.2",
    "bootstrap-icons": "^1.0.0",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@fortawesome/fontawesome-free": "^5.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.0",
    "@angular-devkit/build-ng-packagr": "~0.1001.0",
    "@angular/cli": "~10.1.0",
    "@angular/compiler-cli": "~10.1.0",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
